{
  "name": "leadingsystems/merconis-languageselector",
  "description": "Merconis language selector bundle for Contao 4",
  "keywords": [
    "contao",
    "leading systems",
    "merconis",
    "bundle"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.13 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\LanguageSelectorBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\LanguageSelectorBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}